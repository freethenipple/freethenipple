{
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react"
  ],
  "env" : {
    "es6": true,
    "browser": true
  },
  "plugins": ["prettier", "react"],
  "rules": {
    "prettier/prettier": "error",
    "react/prefer-stateless-function": ["off"],
    "react/jsx-filename-extension": ["off"]

  }
}
